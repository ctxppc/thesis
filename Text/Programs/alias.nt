(value(letType(
	alias(Sequence, cap(vector(of: s32, sealed: false))), in:
	let(
		(sumOfFirstAndSecond, Î»(takes: (sequence, Sequence), returns: s32, in:
			value(binary(element(of: sequence, at: 0), add, element(of: sequence, at: 1)))
		))
		(pi, vector(0, count: 3)),
